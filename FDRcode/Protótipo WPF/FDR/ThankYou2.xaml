<Page x:Class="FDR.ThankYou2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FDR"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="300"
      Title="ThankYou2">

    <Grid Background="White">
        <Label HorizontalContentAlignment="Center" Content="THANK YOU!" Margin="32,117,32,0" VerticalAlignment="Top" Height="51" FontWeight="Bold" FontSize="23"/>
        <Button x:Name="ButtonLogIn_Copy3" Background ="RoyalBlue" Content="HOME PAGE" Margin="26,439,26,0" FontFamily="Arial" VerticalAlignment="Top" Click="ButtonLogIn_Copy3_Click" Foreground="White" FontWeight="Bold" Height="30" FontSize="13">
            <Button.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="5"/>
                </Style>
            </Button.Resources>
        </Button>
    </Grid>
</Page>
